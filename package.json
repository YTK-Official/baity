{
  "name": "baity-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start --turbopack",
    "lint": "next lint",
    "format": "bun biome format --write .",
    "format:check": "bun biome check --fix",
    "better-auth:generate": "bunx @better-auth/cli generate --config=./src/lib/auth/index.ts --output=./src/db/schemas/auth.ts --y",
    "better-auth:secret": "bunx @better-auth/cli secret",
    "db:push": "bun drizzle-kit push",
    "db:generate": "bun drizzle-kit generate",
    "db:migrate": "bun drizzle-kit migrate",
    "db:studio": "bun drizzle-kit studio",
    "db:check": "bun drizzle-kit check",
    "db:seed": "bun run src/db/seed.ts",
    "db:drop": "bun run src/scripts/drop-tables.ts",
    "db:reset": "bun run src/scripts/db-reset.ts",
    "cy:open": "cypress open",
    "ci": "start-server-and-test start http://localhost:3000 cy:open",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "dependencies": {
    "@better-auth/stripe": "^1.2.7",
    "@faker-js/faker": "^9.7.0",
    "@heroui/modal": "^2.2.16",
    "@heroui/react": "2.7.5",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-slot": "^1.2.0",
    "@t3-oss/env-nextjs": "^0.13.4",
    "ahooks": "^3.8.4",
    "better-auth": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "cloudinary": "^2.6.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.41.0",
    "drizzle-seed": "^0.3.1",
    "framer-motion": "12.6.3",
    "lucide-react": "^0.503.0",
    "next": "15.3.2",
    "next-intl": "^4.1.0",
    "nodemailer": "^6.10.1",
    "pg": "^8.14.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.56.0",
    "react-icons": "^5.5.0",
    "react-use-measure": "^2.1.7",
    "recharts": "^2.15.3",
    "stripe": "17.7.0",
    "swiper": "^11.2.6",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "validator-better-auth": "^2.2.4",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^20.17.30",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.13",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "autoprefixer": "^10.4.21",
    "cypress": "14.4.0",
    "drizzle-kit": "^0.30.6",
    "postcss": "^8.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "3.4",
    "typescript": "^5.8.3"
  },
  "trustedDependencies": ["@heroui/shared-utils", "esbuild", "sharp", "unrs-resolver"],
  "pnpm": {
    "ignoredBuiltDependencies": ["@heroui/shared-utils", "esbuild", "sharp", "unrs-resolver"]
  },
  "overrides": {
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3"
  }
}
